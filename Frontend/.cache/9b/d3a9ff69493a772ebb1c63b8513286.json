egories.js","original":{"line":35,"column":12}},{"generated":{"line":118,"column":10},"source":"src/components/Categories/categories.js","original":{"line":34,"column":8}},{"generated":{"line":119,"column":0},"source":"src/components/Categories/categories.js","original":{"line":53,"column":5}},{"name":"Component","generated":{"line":123,"column":2},"source":"src/components/Categories/categories.js","original":{"line":5,"column":25}},{"name":"Categories","generated":{"line":125,"column":15},"source":"src/components/Categories/categories.js","original":{"line":56,"column":15}}],"sources":{"src/components/Categories/categories.js":"import axios from \"axios\";\r\nimport React, { Component }from \"react\";\r\nimport CategoryImg from \"./images/category.jpg\"\r\n\r\nclass Categories extends Component{\r\n    constructor(props) {\r\n        super(props);\r\n        this.state = {\r\n            categories: []\r\n        }\r\n        this.navigateProductPage = this.navigateProductPage.bind(this);\r\n    }\r\n\r\n    navigateProductPage(e, categoryId) {\r\n        window.location = `/productForCategory/${categoryId}`\r\n    }\r\n\r\n\r\n    componentDidMount(){\r\n        axios.get('http://localhost:8087/category/')\r\n            .then(response => {\r\n                this.setState({ categories: response.data.data })\r\n                console.log(response.data.data)\r\n\r\n            })\r\n            .catch(error => {\r\n                console.log(error.message)\r\n                alert(error.message)\r\n            })\r\n\r\n    }\r\n\r\n    render(){\r\n        return(\r\n            <div className=\"container\">\r\n                <center><h1>All Categories</h1></center>\r\n\r\n\r\n                <div style={{display: 'flex', flexDirection: 'row'}}>\r\n                    {this.state.categories.length > 0 && this.state.categories.map((item,index) => (\r\n                        <div className=\"card\" key={index} style={{width: \"18rem\" ,  margin: \"10px\"}}>\r\n                           <img src={CategoryImg} className=\"card-img-top\" alt=\"...\"/>\r\n                            <div className=\"card-body\">\r\n                                <h5 className=\"card-title\">{item.name}</h5>\r\n                                <p className=\"card-text\">Description : {item.description}</p>\r\n                                <a href=\"#\" className=\"btn btn-primary\" onClick={e => this.navigateProductPage(e, item._id)}>Show the Products</a>\r\n                            </div>\r\n                        </div>\r\n                    ))}\r\n                </div>\r\n            </div>\r\n        )\r\n    }\r\n}\r\n\r\nexport default Categories;\r\n"},"lineCount":null}},"error":null,"hash":"670c26c9588d178ab3c63bd2ac8001f5","cacheData":{"env":{}}}